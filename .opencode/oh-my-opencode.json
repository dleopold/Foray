{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "google_auth": false,
  "git_master": {
    "commit_footer": false,
    "include_co_authored_by": false
  },
  "agents": {
    "sisyphus": {
      "model": "opencode/kimi-k2.5-free"
    },
    "prometheus": {
      "model": "opencode/kimi-k2.5-free"
    },
    "oracle": {
      "model": "opencode/gpt-5.2-codex"
    },
    "librarian": {
      "model": "opencode/glm-4.7-free",
      "prompt_append": "Use Context7 for Flutter/Riverpod/Drift docs."
    },
    "explore": {
      "model": "opencode/claude-haiku-4-5"
    },
    "multimodal-looker": {
      "model": "opencode/gemini-3-flash"
    },
    "metis": {
      "model": "opencode/glm-4.7-free"
    },
    "momus": {
      "model": "opencode/glm-4.7-free"
    },
    "atlas": {
      "model": "opencode/gpt-5.2-codex"
    }
  },
  "categories": {
    "visual-engineering": {
      "model": "opencode/gemini-3-pro"
    },
    "ultrabrain": {
      "model": "opencode/gpt-5.2-codex",
      "variant": "xhigh"
    },
    "artistry": {
      "model": "opencode/gemini-3-flash"
    },
    "quick": {
      "model": "opencode/claude-haiku-4-5"
    },
    "unspecified-low": {
      "model": "opencode/claude-sonnet-4-5"
    },
    "unspecified-high": {
      "model": "opencode/gpt-5.2-codex",
      "variant": "high"
    },
    "writing": {
      "model": "opencode/gemini-3-flash"
    }
  },

  "background_task": {
    "defaultConcurrency": 5,
    "modelConcurrency": {
      "opencode/claude-opus-4-5": 2,
      "opencode/gpt-5-nano": 10,
      "opencode/glm-4.7-free": 8,
      "opencode/kimi-k2.5-free": 4
    }
  },
  "lsp": {
    "marksman": {
      "command": [
        "marksman",
        "server"
      ],
      "extensions": [
        ".md",
        ".mdx"
      ]
    },
    "dart": {
      "command": [
        "dart",
        "language-server"
      ],
      "extensions": [
        ".dart"
      ],
      "priority": 10
    }
  }
}
